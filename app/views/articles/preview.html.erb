<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <%= csrf_meta_tags %>
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
</head>

<body>
  <section class="container">
    <div class="content-wrap">
      <div class="content" style="margin: 0 auto; max-width: 820px; margin-top: 15px;">
        <%= render :partial => "articles/shared/article_show", locals: {article: @article} %>
      </div>
    </div>
  </section>
</body>
</html>